from random import randint

n = 10
v = []
i = 0

def somatotal(lista):
    soma = 0
    for i in lista:
        soma += i
    return soma

print(n)
while i <= n:
    for i in range(0, 5):
        i = i + 1
        v.append(i)
    if somatotal == n:
        break

print(somatotal(v))
print(v)